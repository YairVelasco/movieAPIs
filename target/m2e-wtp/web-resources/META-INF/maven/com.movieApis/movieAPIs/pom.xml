		<!-- JDBC version -->
		<jdbc.version>4.3.11.RELEASE</jdbc.version>

		<!--Hibernate version -->
		<hibernate.version>4.3.0.Final</hibernate.version>

		<!-- Spring version -->
		<spring.version>4.0.1.RELEASE</spring.version>
